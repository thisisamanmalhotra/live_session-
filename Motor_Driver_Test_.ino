int A = 9; 
int B = 8;   
int C = 7;
int D = 6;     
void setup()
{
  pinMode(A, OUTPUT);
  pinMode(B, OUTPUT);
  pinMode(C, OUTPUT);
  pinMode(D, OUTPUT);
}

void loop()
 {
  digitalWrite(A,HIGH)
  delay(300);
  digitalWrite(B,HIGH)
  delay(300);
  digitalWrite(C,HIGH)
  delay(300);
  digitalWrite(D,HIGH)
  delay(300);

}
